<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>AccServlet</servlet-name>
        <servlet-class>com.incubator.task.servlet.AccServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>RichServlet</servlet-name>
        <servlet-class>com.incubator.task.servlet.RichServlet</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>AccServlet</servlet-name>
        <url-pattern>/accsum</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>RichServlet</servlet-name>
        <url-pattern>/richest</url-pattern>
    </servlet-mapping>

</web-app>